{
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Tabs to Window",
    "default_popup": "tabs.html"
  },
  "description": "Move multiple tabs.",
  "icons": {
    "48": "icon-48.png"
  },
  "homepage_url": "https://github.com/pmarchwiak/tabs-to-window",
  "manifest_version": 2,
  "name": "Tabs to Window",
  "permissions": [
    "tabs", "menus"
  ],
  "version": "1.0"
}
