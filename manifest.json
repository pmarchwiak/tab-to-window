{
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
      "scripts": ["background.js"]
  },
  "description": "Move multiple tabs to another window.",
  "icons": {
    "48": "icon-48.png"
  },
  "homepage_url": "https://github.com/pmarchwiak/tabs-to-window",
  "manifest_version": 2,
  "name": "Tabs to Window",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "menus", 
    "storage", 
    "tabs"
  ],
  "version": "1.0"
}
